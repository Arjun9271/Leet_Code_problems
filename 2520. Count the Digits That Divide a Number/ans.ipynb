    "# count the digits that divide a number:\n",
    "\n",
    "def cnt_div_number(n):\n",
    "    temp = n\n",
    "    count = 0\n",
    "    while temp > 0:\n",
    "        last_digit = temp%10\n",
    "        if n%last_digit == 0:\n",
    "            count += 1\n",
    "        temp //= 10\n",
    "    return count\n",
    "ans = cnt_div_number(1248)\n",
    "print(ans)"
   ]
