    "def is_valid_square(n):\n",
    "    l = 0\n",
    "    r = n\n",
    "    while l <= r:\n",
    "        mid = (l+r)//2\n",
    "        \n",
    "        if mid*mid == n:\n",
    "            return True\n",
    "        if mid*mid > n:\n",
    "            r = mid-1\n",
    "        else:\n",
    "            l = mid+1\n",
    "    return False\n",
    "\n",
    "result = is_valid_square(15)\n",
    "print(result)"
   ]
